$ORIGIN %(dns_domain)s.
$TTL 86400 

@	IN	SOA	%(nameserver_hostname)s.	%(dns_admin_email)s. (
			%(dns_serial_no)s ; serial
			21600      ; refresh after 6 hours
			3600       ; retry after 1 hour
			604800     ; expire after 1 week
			86400 )    ; minimum TTL of 1 day


	IN	NS	%(nameserver_hostname)s.

%(host_records)s
